# Evolution OpenAI Environment Variables
# Скопируйте этот файл в .env и заполните ваши реальные значения

# Evolution API Credentials
EVOLUTION_KEY_ID=your_key_id_here
EVOLUTION_SECRET=your_secret_here

# Evolution OpenAI Endpoint
# Для обычных примеров используйте ваш рабочий endpoint
EVOLUTION_BASE_URL=https://your-endpoint.cloud.ru/v1

# Evolution Foundation Models Endpoint (опционально)
# Если установлен, то будет использован для Foundation Models примеров
EVOLUTION_FOUNDATION_MODELS_URL=https://foundation-models.api.cloud.ru/api/gigacube/openai/v1

# Evolution Foundation Models Project ID (опционально)
EVOLUTION_PROJECT_ID=your_project_id_here

# Token Service Endpoint (обычно не нужно менять)
EVOLUTION_TOKEN_URL=https://iam.api.cloud.ru/api/v1/auth/token

# Test Configuration
# Установите в true для запуска интеграционных тестов с реальным API
ENABLE_INTEGRATION_TESTS=false

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO